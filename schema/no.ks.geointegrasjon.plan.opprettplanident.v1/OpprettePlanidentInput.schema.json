{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://fiks.ks.no/geointegrasjon.plan.oppretteplanidentinput.v1.schema.json",
  "title": "no.ks.geointegrasjon.plan.oppretteplanidentinput.v1",
  "description": "Melding for å få tildelt ny arealplanId",
  "type": "object",
  "properties": {
    "plannavn": {
      "description": "Navn på planen",
      "type": "string"
    },
    "plantype": {
      "description": "Type plan",
      "type": "object",
      "properties": {
        "kodeverdi": {
          "type": "string"
        },
        "kodebeskrivelse": {
          "type": "string"
        }
      }
    },
    "planbestemmelse": {
      "description": "Type planbestemmelse",
      "type": "object",
      "properties": {
        "kodeverdi": {
          "type": "string"
        },
        "kodebeskrivelse": {
          "type": "string"
        }
      }
    },
    "lovreferanse": {
      "description": "Referanse til lov",
      "type": "object",
      "properties": {
        "kodeverdi": {
          "type": "string"
        },
        "kodebeskrivelse": {
          "type": "string"
        }
      }
    }
  },
  "required": [ "plannavn", "plantype" ]
}